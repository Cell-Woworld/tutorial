        {
          "id": "app_registration_password1",
          "component": "v-text-field",
          "label": "::Learning.Language[::Language][title_password]",
          "style": "margin:30px 10% 0px;",
          "type": "password"
        },
        {
          "id": "app_registration_password2",
          "component": "v-text-field",
          "label": "::Learning.Language[::Language][title_password2]",
          "style": "margin:30px 10% 0px;",
          "type": "password"
        },
        {
          "component": "v-text-field",
          "id": "app_registration_userName",
          "label": "::Learning.Language[::Language][title_userName](15::Learning.Language[::Language][words_limit])",
