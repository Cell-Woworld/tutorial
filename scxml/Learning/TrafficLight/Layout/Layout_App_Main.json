{
  "tutorial1": [
    {
      "id": "app_trafficLight_stop",
      "component": "IconButton",
      "iconized": true,
      "icon": "mdi-stop",
      "size": "30",
      "style": "position:absolute;top:2px;right:5px;height:34px;width:34px;color:orange;"
    },
    {
      "id": "app_trafficLight_pause",
      "component": "IconButton",
      "iconized": true,
      "icon": "mdi-pause",
      "size": "30",
      "style": "position:absolute;top:2px;right:45px;height:34px;width:34px;color:orange;",
      "invisible": "::(::Learning.TrafficLightView.isPlaying==true?'false':'true')"
    },
    {
      "id": "app_trafficLight_play",
      "component": "IconButton",
      "iconized": true,
      "icon": "mdi-play",
      "size": "30",
      "style": "position:absolute;top:2px;right:45px;height:34px;width:34px;color:orange;",
      "invisible": "::(::Learning.TrafficLightView.isPlaying==true?'true':'false')"
    },
    {
      "id": "app_trafficLight",
      "component": "Section",
      "subcomponent": "div",
      "style": "margin:40px 5% 0px;max-height:calc(100vh - 60px - 30px);overflow:hidden auto;border-radius:30px;background-color:white;",
      "items": [
        {
          "id": "app_trafficLight_red",
          "component": "div",
          "style":"::Learning.TrafficLightView.RedStyle"
        },
        {
          "id": "app_trafficLight_yellow",
          "component": "div",
          "style":"::Learning.TrafficLightView.YellowStyle"
        },
        {
          "id": "app_trafficLight_green",
          "component": "div",
          "style":"::Learning.TrafficLightView.GreenStyle"
        }
      ]
    }
  ],
