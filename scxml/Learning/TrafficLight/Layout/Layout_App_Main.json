        },
        {
          "id": "app_trafficLight_note",
          "component": "RichText",
          "content": "::Learning.Language[::Language][down_sourceCode_note]",
          "style": "clear:both;padding:30px 5% 0px calc(5% + 1em);text-align:left;text-indent: -1em;"
        },
        {
          "component": "input",
          "id": "app_trafficLight_pick_file",
          "type": "file",
          "invisible": "true",
          "accept": ".scxml",
          "multiple": false
        }
      ]
    }
